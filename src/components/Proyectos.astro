import catriel from "../assets/proyectos/catriel.png";
import franzizca from "../assets/proyectos/franzizca.png";
import no_te_vayas from "../assets/proyectos/NOTEVAYAS.png";
import minerva from "../assets/proyectos/minerva.png";
