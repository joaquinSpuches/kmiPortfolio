{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "moduleResolution": "bundler",
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    ".astro/types.d.ts",
    "src/**/*",
    "src/env.d.ts"
  ],
  "exclude": ["dist"]
}